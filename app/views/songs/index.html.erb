<h1 id="my_jams">My Jams</h1>
  <%= link_to '<button id="create_song" class="mdl-button mdl-js-button mdl-button--raised mdl-button--colored">Create New Song</button>'.html_safe, new_song_path %></button>

<div class="demo-grid-1 mdl-grid">
    <% @songs.each do |song| %>
      <div class="mdl-cell mdl-cell--4-col">
        <div class="mdl-card mdl-shadow--2dp demo-card-wide">
          <div class="mdl-card__title">
            <h3 class="mdl-card__title-text"><%= song.artist %></h3>
          </div>
          <div class="mdl-card__media">
            <i id="song_link"><%= link_to song.title, song_path(song) %></i>
          </div>
          <div class="mdl-card__actions mdl-card--border">
              <iframe width="100%" height="200" scrolling="no" frameborder="no" src='<%= song.sound_cloud %>'></iframe>
          </div>
          <div class="mdl-card__menu">
            <button class="mdl-button mdl-button--icon mdl-js-button mdl-js-ripple-effect">
              <i class="material-icons">share</i>
            </button>
          </div>
        </div>
      </div>
<% end %>
</div>


